{
  "name": "shopme",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "jest": {
    "testMatch": [
      "**/?(*.)+(spec|test).[tj]s?(x)"
    ],
    "watchPathIgnorePatterns": [
      "/__tests__/test-utils",
      "/__tests__/mocks",
      "/__tests__/types"
    ]
  },
  "dependencies": {
    "@auth0/auth0-react": "1.11.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "13.3.0",
    "@types/nock": "11.1.0",
    "@types/node": "16.0.0",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "axios": "0.27.2",
    "cypress": "10.10.0",
    "dotenv": "16.0.3",
    "env-cmd": "10.1.0",
    "nock": "13.2.9",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-axios": "2.0.6",
    "react-dom": "18.2.0",
    "react-redux": "8.0.2",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1",
    "react-toastify": "9.0.8",
    "rimraf": "5.0.1",
    "serve": "14.0.1",
    "supertest": "6.2.4",
    "typescript-axios": "1.0.0",
    "web-vitals": "2.1.4"
  },
  "scripts": {
    "dev": "react-scripts start",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "start:dev": "env-cmd -f .env.development npm start",
    "start:e2e": "env-cmd -f .env.production npm run build",
    "start:prod": "env-cmd -f .env.production npm run build",
    "serve:e2e": "serve -s build",
    "test": "env-cmd -f .env.development react-scripts test --verbose",
    "test:dev": "env-cmd -f .env.development react-scripts test --verbose",
    "test:prod": "env-cmd -f .env.production react-scripts test --verbose",
    "cypress": "cypress open",
    "test:e2e": "npx cypress run --spec cypress/e2e/tests/*.cy.ts",
    "coverage": "react-scripts test --env=jsdom --watchAll=false --coverage",
    "coverage:full": "jest --verbose --coverage --coverageReporters='text-summary'",
    "format:check": "npx prettier --check .",
    "format:fix": "npx prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "eject": "react-scripts eject",
    "heroku-postbuild": "npm run build",
    "prepare": "husky install",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.22.5",
    "@babel/preset-env": "7.22.5",
    "@babel/preset-typescript": "7.22.5",
    "@types/jest": "29.0.0",
    "@types/react-redux": "7.1.24",
    "@types/redux-mock-store": "1.0.3",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.40.1",
    "babel-jest": "29.0.0",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard-with-typescript": "23.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.3.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.0",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.0",
    "jest": "29.0.0",
    "jest-watch-typeahead": "2.2.2",
    "prettier": "2.7.1",
    "redux-devtools-extension": "2.13.9",
    "redux-mock-store": "1.5.4",
    "ts-jest": "29.0.0",
    "typescript": "4.8.4"
  }
}
